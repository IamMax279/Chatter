server:
    port: 8080

spring:
  application:
    name: chatter_gateway
  cloud:
    gateway:
      routes:
        server:
          webflux:
            - id: auth_service
              uri: http://localhost:8081
              predicates:
                - Path=/auth/**
jwt:
  secret:
    key: <JWT_SECRET_KEY>

service:
  url:
    auth: ""
    user: ""